{
    "tracker_right_map": {
        "type": "container",
        "background": "#212121",
        "content": {
            "type": "dock",
            "dropshadow": "true",
            "content": [
                {
                    "type": "dock",
                    "dock": "left",
                    "orientation": "horizontal",
                    "v_alignment": "stretch",
                    "margin": "0,0,3,0",
                    "content": [
                        {
                            "header_content": {
                                "type": "button_popup",
                                "style": "settings",
                                "popup_background": "#FF212121",
                                "layout": "options"
                            },
                            "type": "group",
                            "header": "Items",
                            "dock": "top",
                            "content": {
                                "type": "layout",
                                "key": "item_grid"
                            }
                        }
                    ]
                },
                {
                    "type": "dock",
                    "dock": "bottom",
                    "orientation": "horizontal",
                    "v_alignment": "stretch",
                    "margin": "0,0,3,0",
                    "content": [
                        {
                            "type": "group",
                            "header": "Pinned Locations",
                            "content": {
                                "type": "recentpins",
                                "style": "wrap",
                                "h_alignment": "stretch",
                                "v_alignment": "stretch",
                                "orientation": "vertical",
                                "compact": true
                            }
                        }
                    ]
                },
                {
                    "type": "dock",
                    "content": [
                        {
                            "type": "map",
                            "maps": [
                                "map"
                            ]
                        }
                    ]
                }
            ]
        }
    }
}